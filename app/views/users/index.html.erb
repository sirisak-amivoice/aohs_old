
<div align='center'><%= will_paginate(@users) %></div>
<table>
  <tr>
    <th>Id</th>
    <th>Display Name</th>
    <th>Account Name</th>
    <th>Type</th>
    <th>Group</th>
    <th>Permission</th>
    <th>Lock version</th>
  </tr>

<% for user in @users %>
  <tr>
    <td><%=h user.id %></td>
    <td><%=h user.display_name %></td>
    <td><%=h user.login %></td>
    <td><%=h user.type %></td>
    <td><%=h user.group.name %></td>
    <td><%=h user.permission_id %></td>
    <td><%=h user.lock_version %></td>
    <td><%= link_to 'Show', :action=>"show", :id=>user.id %></td>
    <td><%= link_to 'Edit', :action=>"edit", :id=>user.id %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New user', :action=>"new" %>
