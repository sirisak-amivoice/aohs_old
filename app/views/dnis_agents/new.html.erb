<% content_for :header do %>

<script type="text/javascript">
</script>
<% end %>

<% content_for :submenu do %>
  <div id="submenu">
    <table width="100%">
      <tr>
        <td align="left">
          <ul>
            <li><%= link_image_tag('Back','back.png',{:controller => 'dnis_agents',:action => 'index'},{:title => 'Back to DnisAgent list'}) %></li>
          </ul>
        </td>
        <td align="right">

        </td>
      </tr>
    </table>
  </div>
<% end %>

<div id="div-block-dnisagent">

<div class="div-form-default-style1">
  <table class="tblfrm" width="100%" cellpadding="0" cellspacing="0">
    <tr>
      <td><span class="span-form-title">New DnisAgent</span></td>
    </tr>
    <tr>
      <td><span class="span-form-name">DnisAgent infomation</span></td>
    </tr>
    <tr>
      <td>
        <% form_for :dnis_agent, :url => {:controller => 'dnis_agents', :action => 'create' } do |f| %>
        <%=display_frm_error(flash[:message]) %>
          <table class="tbl-frm-details" cellpadding="0" cellspacing="0">
            <tr>
              <td width="150px" valign="top"><label for="login">DNIS :</label></td>
              <td><%= f.text_field :dnis, :size => 10, :maxsize => 10, :class =>'input-text' %><span class="span-require">*</span></td>
            </tr>
            <tr>
              <td width="150px" valign="top"><label for="login">CTI Login :</label></td>
              <td><%= f.text_field :ctilogin, :size => 10, :maxsize => 10, :class =>'input-text' %><span class="span-require">*</span></td>
            </tr>
            <tr>
              <td width="150px" valign="top"><label for="login">Team :</label></td>
              <td><%= f.text_field :team, :size => 10, :maxsize => 10, :class =>'input-text' %><span class="span-require">*</span></td>
            </tr>						
            <tr>
              <td></td>
              <td><%= submit_tag "Add DnisAgent",{:class => 'btnAccept'} %>&nbsp;&nbsp;<%#= submit_tag "Reset",{:type => 'Reset',:class => 'btnAccept'} %></td>
            </tr>
            </table>
        <% end %>
      </td>
    </tr>
  </table>
</div>

</div>

